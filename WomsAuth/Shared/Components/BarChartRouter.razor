<Router AppAssembly="@typeof(BarChartRouter).Assembly">
    <Found Context="routeData1">
        <RouteView RouteData="@routeData1" DefaultLayout="@typeof(BarChartLayout)" />
        <FocusOnNavigate RouteData="@routeData1" Selector="h1" />
    </Found>
    <NotFound>
        <LayoutView Layout="@typeof(BarChartLayout)">
            <p role="alert">Sorry, there's nothing at this address.</p>
        </LayoutView>
    </NotFound>
</Router>

